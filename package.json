{
  "name": "school-dashboard-backend",
  "version": "0.1.0",
  "private": true,
  "author": "Rob Boskind",
  "license": "MIT",
  "scripts": {
    "dev": "keystone-next",
    "seed-data": "keystone-next --seed-data",
    "start": "keystone-next ",
    "build": "keystone-next build",
    "deploy": "keystone-next start"
  },
  "eslintConfig": {
    "extends": "wesbos/typescript.js",
    "rules": {
      "@typescript-eslint/no-unsafe-assignment": 0
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 10,
            "browsers": [
              "last 2 chrome versions",
              "last 2 firefox versions",
              "last 2 safari versions",
              "last 2 edge versions"
            ]
          }
        }
      ],
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  },
  "dependencies": {
    "@keystone-next/admin-ui": "^14.1.3",
    "@keystone-next/auth": "^14.0.0",
    "@keystone-next/cloudinary": "^12.0.0",
    "@keystone-next/fields": "^4.1.1",
    "@keystone-next/keystone": "^9.3.0",
    "@keystone-next/types": "^12.0.0",
    "@keystonejs/server-side-graphql-client": "^2.1.2",
    "@types/nodemailer": "^6.4.4",
    "dotenv": "^16.0.0",
    "next": "^10.0.5",
    "nodemailer": "^6.7.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "stripe": "^8.215.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-wesbos": "^3.0.2",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": "https://github.com/wesbos/advanced-react"
}
